datasource db {
  provider = "cockroachdb"
  // provider = "postgres"
  // url      = "postgresql://postgres@localhost:5432/cardqn?schema=public"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "cargo prisma"
  output   = "../src/db.rs"
}

model Rb {
  id         String   @id @default(cuid())
  created_at DateTime @default(now())
  state      Float[]
  action     Int
  reward     Float
  next_state Float[]
  done       Boolean
}

